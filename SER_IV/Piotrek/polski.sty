\def\Slash{/}
\def\PLSlash#1{%
  \ifx#1\PLSlash \expandafter\Slash
  \else
    \expandafter \ifx \csname PLSlash@\string#1\endcsname \relax
      \PLSlash@error#1%
    \else
      \expandafter\expandafter\expandafter\PLSlash@letter
      \expandafter\expandafter\expandafter#1%
    \fi
  \fi
}

